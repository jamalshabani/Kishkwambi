{
  "expo": {
    "name": "Simba Terminals",
    "slug": "simba-terminals",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "simba-terminals",
    "splash": {
      "image": "./assets/logo/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#BC8405"
    },
    "ios": {
      "supportsTablet": true,
      "backgroundColor": "#BC8405"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo/logo.png",
        "backgroundColor": "#BC8405"
      },
      "backgroundColor": "#BC8405",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/logo/logo.jpg"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
